<a class="wir-link <%= locals.class %>
  <% if (locals.disabled) { %>wir-link--disabled<% } %>
  <% if (locals.isDarkTheme) { %>wir-link--theme-dark<% } %>" href="<%= locals.href %>">
  <span><%- locals.text %></span>
  <% if (locals.rightIcon) { %>
    <span class="wir-link__icon is-pulled-right">
      <i class="fa fa-<%= locals.rightIcon %>"></i>
    </span>
  <% } %>
</a>
