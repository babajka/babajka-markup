{
  "name": "babajka-markup",
  "version": "1.1.1",
  "description": "Markups",
  "scripts": {
    "start": "cross-env PORT=3001 npm run watch",
    "build": "gulp build",
    "watch": "gulp watch",
    "lint": "gulp lint && npm run eslint .",
    "deploy": "cross-env BABAJKA_PREFIX='babajka-markup' npm run build && gh-pages -d dist -a",
    "prerelease": "npm version patch",
    "postrelease": "git push && git push --tags",
    "release": "npm publish",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "eslint": "eslint --ignore-path .gitignore --fix",
    "prettier": "prettier --write",
    "prettier-stylelint": "prettier-stylelint --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babajka/babajka-markup.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/babajka/babajka-markup/issues"
  },
  "homepage": "https://github.com/babajka/babajka-markup#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "bulma": "^0.6.2",
    "bulma-badge": "^2.0.0",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/git": "^7.0.4",
    "babel-eslint": "^10.0.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.4",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fancy-log": "^1.3.2",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat-css": "^3.1.0",
    "gulp-connect": "^5.7.0",
    "gulp-ejs": "^3.1.3",
    "gulp-htmlhint": "^2.2.1",
    "gulp-plumber": "^1.2.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-stylelint": "^7.0.0",
    "gulp-watch": "^5.0.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "prettier-stylelint": "^0.4.2",
    "semantic-release": "^15.9.17",
    "stylelint": "^9.9.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.3",
    "travis-deploy-once": "^5.0.9"
  }
}
