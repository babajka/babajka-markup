{
  "name": "babajka-markup",
  "version": "0.2.5",
  "description": "Markups",
  "scripts": {
    "start": "cross-env PORT=3001 npm run watch",
    "build": "gulp build",
    "watch": "gulp watch",
    "lint": "gulp lint",
    "deploy": "cross-env BABAJKA_PREFIX='babajka-markup' npm run build && gh-pages -d dist -a",
    "prerelease": "npm version patch",
    "postrelease": "git push && git push --tags",
    "release": "npm publish",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "**/*.scss": "stylelint --syntax=scss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babajka/babajka-markup.git"
  },
  "author": "babajka team <github.com/babajka>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/babajka/babajka-markup/issues"
  },
  "homepage": "https://github.com/babajka/babajka-markup#readme",
  "dependencies": {
    "bulma": "^0.5.1",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "fancy-log": "^1.3.2",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat-css": "^3.1.0",
    "gulp-connect": "^5.5.0",
    "gulp-ejs": "^3.1.3",
    "gulp-htmlhint": "^2.1.1",
    "gulp-plumber": "^1.2.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-stylelint": "^7.0.0",
    "gulp-watch": "^5.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "semantic-release": "^11.0.2",
    "stylelint": "^9.2.1",
    "stylelint-config-sass-guidelines": "^5.0.0"
  }
}
